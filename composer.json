{
  "name": "riasage/bahai-enroll-form-divi",
  "description": "Divi Builder module providing a bilingual Bahá’í enrollment form",
  "type": "wordpress-plugin",
  "require": {},
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.1",
    "squizlabs/php_codesniffer": "^3.13",
    "wp-coding-standards/wpcs": "^3.2",
    "phpcsstandards/phpcsextra": "^1.4",
    "phpcsstandards/phpcsutils": "^1.1"
  },
  "scripts": {
    "lint": "php -l $(git ls-files '*.php')",
    "cs": "vendor/bin/phpcs --standard=phpcs.xml.dist"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
